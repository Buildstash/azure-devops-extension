{
  "manifestVersion": 1,
  "id": "buildstash-tasks",
  "name": "Buildstash",
  "version": "1.0.15",
  "publisher": "Buildstash",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Upload build artifacts to Buildstash platform",
  "public": true,
  "categories": [
    "Azure Pipelines"
  ],
  "Tags": [
      "Buildstash",
      "Builds",
      "Build",
      "Archive",
      "Store",
      "Organize",
      "Azure",
      "Release",
      "DevOps",
      "azure-devops",
      "Pipelines",
      "artifacts",
      "upload"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "links": {
    "getstarted": {
      "uri": "https://buildstash.com"
    },
    "learn": {
      "uri": "https://support.buildstash.com/docs/integrations/azure-devops-pipelines"
    },
    "support": {
      "uri": "https://support.buildstash.com"
    },
    "repository": {
      "uri": "https://github.com/Buildstash/azure-devops-extension"
    },
    "privacypolicy": {
      "uri": "https://buildstash.com/privacy"
    }
  },
  "repository": {
      "type": "git",
      "uri": "https://github.com/Buildstash/azure-devops-extension"
  },
  "files": [
    {
      "path": "tasks/BuildstashUpload"
    }
  ],
  "contributions": [
    {
      "id": "buildstash-upload",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "tasks/BuildstashUpload"
      }
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  }
} 